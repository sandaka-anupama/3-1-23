n=int(input())
A=[]
sum=0
sum1=0
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    A.append(l)
    
for i in range(n):
    for j in range(n):
        if i+j==n-1:
            sum=sum+A[i][j]
        if i==j:
            sum1=sum1+A[i][j]
print(sum-sum1)

output:
3
1
2
10
20
13
17
18
19
2
25
